# Telegram Bot Token from @BotFather
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Your Telegram User ID (only this user can interact with the bot)
# You can get your ID by messaging @userinfobot on Telegram
TELEGRAM_USER_ID=your_user_id_here

# Discord Bot Token from Developer Portal
DISCORD_BOT_TOKEN=your_bot_token_here

# Your Discord User ID (only this user can interact with the bot)
DISCORD_USER_ID=your_user_id_here

# Optional - uncomment to override defaults
# OLLAMA_HOST=http://localhost:11434
# OLLAMA_CHAT_MODEL=gpt-oss:20b
# OLLAMA_EMBED_MODEL=nomic-embed-text
